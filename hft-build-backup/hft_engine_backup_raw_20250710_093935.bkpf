BACKUP_DATE=$(date '+%Y-%m-%d %H:%M:%S')

# Create the complete JSON structure
JSON_CONTENT=\"{
  \\\"metadata\\\": {
    \\\"projectName\\\": \\\"$(escape_json \"$PROJECT_NAME\")\\\",
